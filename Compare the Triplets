#include<bits/stdc++.h>
using namespace std;
int main()
{
    long int a[100],c=0,ct=0,s=0,b[100];

    for(int i=0;i<3;i++)
    {
        cin>>a[i];
    }
    for(int i=0;i<3;i++)
    {
        cin>>b[i];
    }

        for(int i=0;i<3;i++)
{
    if(a[i]>b[i])
    {
        c++;
    }
   else if(a[i]<b[i])
    {
        ct++;
    }
}
cout<<c<<" "<<ct;

}
